{
  "name": "packard-belle",
  "version": "0.2.0",
  "description": "Library of components to mimic old desktop",
  "main": "build/pb.js",
  "module": "build/pb.module.js",
  "repository": "https://github.com/padraigfl/packard-belle",
  "scripts": {
    "build:module": "rollup -c rollup.config.babel.js",
    "watch": "rollup -c rollup.config.babel.js -w",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "build-storybook -c .storybook -o .out",
    "build:css": "node-sass src/_scss/w98_simple.scss -o build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --no-cache --watch",
    "coverage": "jest --env=jsdom --coverage",
    "coveralls": "npm run coverage | coveralls",
    "lint": "eslint -c .eslintrc.js --quiet --ext .js './src/**/*.js'",
    "lint:css": "stylelint 'src/**/*.scss'",
    "precommit-build": "npm run build:module && git add build"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "windows",
    "desktop",
    "98"
  ],
  "author": "padraigf (https://github.com/padraigfl)",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "clone": "^2.1.2",
    "prop-types": "^15.6.2",
    "react-select": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-notes": "^4.0.4",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/addon-storysource": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "ajv": "^6.5.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^24.1.0",
    "node-sass": "^4.9.2",
    "pre-commit": "^1.2.2",
    "raf": "^3.4.0",
    "react": "~16.7.0",
    "react-dom": "~16.7.0",
    "react-test-renderer": "~16.7.0",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.63.5",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.10.1",
    "stylelint-scss": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "pre-commit": [
    "lint",
    "lint:css",
    "build:css",
    "build",
    "precommit-build"
  ]
}
